{
  "$schema" : "http://json-schema.org/draft-04/schema#",
  "title" : "Reference Link Group",
  "type" : "object",
  "additionalProperties" : false,
  "properties" : {
    "id" : {
      "type" : "string"
    },
    "folioReference" : {
      "type" : "string"
    },
    "externalReference" : {
      "type" : "string"
    },
    "type" : {
      "$ref" : "#/definitions/ReferenceLinkType"
    },
    "references" : {
      "type" : "array",
      "items" : {
        "type" : "string"
      }
    }
  },
  "required" : [ "folioReference", "externalReference", "type" ],
  "definitions" : {
    "ReferenceLinkType" : {
      "type" : "object",
      "additionalProperties" : false,
      "properties" : {
        "id" : {
          "type" : "string"
        },
        "name" : {
          "type" : "string"
        }
      },
      "required" : [ "name" ]
    }
  }
}